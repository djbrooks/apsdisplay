# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles, tempfiles, and vim editor swap files.
# Also, local help info in .README.txt for developers who forget things... :) -djb 
*.swp
.README.txt
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

/node_modules
/yarn-error.log

.byebug_history
